{
  "name": "dntsc-api",
  "version": "0.0.0",
  "description": "REST API Node monolith app for DNTSC project",
  "author": "Alexandr Changli",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "start:dev": "docker run -d -p 27017:27017 -v ~/dntsc_dev_mongo_db_data:/data/db mongo & nodemon"
  },
  "dependencies": {
    "@nestjs/common": "^5.4.0",
    "@nestjs/core": "^5.4.0",
    "@nestjs/jwt": "^0.2.0",
    "@nestjs/mongoose": "^5.2.2",
    "@nestjs/passport": "^5.1.0",
    "@types/bcryptjs": "^2.4.2",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "date-fns": "^1.30.1",
    "dotenv": "^6.1.0",
    "express-rate-limit": "^3.3.2",
    "helmet": "^3.15.0",
    "mongodb": "^3.1.10",
    "mongoose": "^5.3.14",
    "nest-access-control": "^1.0.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.2.2",
    "typescript": "^3.0.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.1.0",
    "@types/express": "^4.16.0",
    "@types/express-rate-limit": "^3.3.0",
    "@types/helmet": "^0.0.42",
    "@types/jest": "^23.3.1",
    "@types/mongoose": "^5.3.3",
    "@types/node": "^10.7.1",
    "@types/passport": "^0.4.7",
    "@types/passport-jwt": "^3.0.1",
    "@types/supertest": "^2.0.5",
    "jest": "^23.5.0",
    "kill-port": "^1.3.2",
    "nodemon": "^1.18.6",
    "prettier": "^1.14.2",
    "rxjs-tslint-rules": "^4.14.4",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "5.11.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-immutable": "^5.1.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
